{
  "name": "amazon-bookmarklet",
  "version": "1.0.0",
  "description": "A bookmarklet to generate a shotened URL from Amazon (optionally with title).",
  "main": "",
  "scripts": {
    "bookmarklet": "bookmarklet",
    "build:url": "bookmarklet url.js dest/url.txt",
    "build:url-title": "bookmarklet url-title.js dest/url-title.txt",
    "build:af-url": "bookmarklet af-url.js dest/af-url.txt",
    "build:af-url-title": "bookmarklet af-url-title.js dest/af-url-title.txt",
    "build:markdown": "bookmarklet markdown.js dest/markdown.txt",
    "build:af-markdown": "bookmarklet af-markdown.js dest/af-markdown.txt",
    "build": "npm-run-all -p build:*"
  },
  "repository": {
    "type": "git",
    "url": "amazon-bookmarklet"
  },
  "author": "Yuki Fujiwara",
  "license": "MIT",
  "devDependencies": {
    "bookmarklet": "0.0.6",
    "npm-run-all": "^4.1.1"
  }
}
